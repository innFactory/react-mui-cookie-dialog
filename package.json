{
  "version": "2.0.0",
  "license": "MIT",
  "description": "A simple solution for a GDPR compliant Cookie dialog with support for multiple Cookie categories.",
  "keywords": [
    "react",
    "material-ui",
    "cookies",
    "cookie-dialog",
    "cookie-categories",
    "gdpr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/innFactory/react-mui-cookie-dialog"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {}
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-mui-cookie-dialog",
  "author": "Nicolas Schlecker (nicolasschlecker@gmail.com)",
  "module": "dist/react-mui-cookie-dialog.esm.js",
  "size-limit": [
    {
      "path": "dist/react-mui-cookie-dialog.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-mui-cookie-dialog.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.1",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "husky": "^4.3.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {}
}
